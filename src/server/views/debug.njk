  <script src="https://cdn.jsdelivr.net/npm/jquery.json-viewer@1.5.0/json-viewer/jquery.json-viewer.min.js"></script>
  <style>
    a.json-toggle::before {
      line-height: 1.5;
    }

    .json-string {
      word-break: break-all;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1 class="text-center">ðŸ’¥</h1>
    <h3 class="text-center">
      {{ message }}
    </h3>
    <section
      id="details"
      class="bg-light border border-secondary rounded"
    />
  </div>
  <script>
    $('#details').jsonViewer({{ details | safe }}, {
      collapsed: true,
      withLinks: true,
      rootCollapsable: false,
    });
  </script>
</body>
</html>
