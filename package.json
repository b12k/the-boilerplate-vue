{
  "name": "vueltimate-boilerplate",
  "version": "1.0.0",
  "description": "Vueltimate Boilerplate - VUE + EXPRESS + SSR + HMR",
  "main": "./dist/server.js",
  "keywords": [],
  "author": "Bogdan Kolesnyk",
  "license": "ISC",
  "scripts": {
    "dev": "npm run clean && env-cmd run-p dev:*",
    "start:local": "npm run clean && env-cmd cross-env NODE_ENV=production SERVER_ENV=production run-p build:* && env-cmd cross-env NODE_ENV=production PORT=80 node --inspect ./dist/server.js",
    "build": "npm run clean && env-cmd run-p build:*",
    "clean": "del-cli dist/**/*",
    "lint": "run-p lint:*",
    "start": "node ./dist/server.js",
    "build:browser": "webpack --config webpack/config.browser.js",
    "build:server": "cross-env BABEL_ENV=node babel src/server --out-dir dist --copy-files",
    "build:ssr": "webpack --config webpack/config.ssr.js",
    "dev:browser": "webpack-dev-server --config webpack/config.browser.js",
    "dev:server": "cross-env BABEL_ENV=node nodemon src/server/server.js",
    "dev:ssr": "webpack --config webpack/config.ssr.js --watch",
    "lint:scripts": "eslint ./ --ext .js,.vue,.html",
    "lint:styles": "stylelint **/*.css **/*.scss **/*.html !./dist/**",
    "heroku-postbuild": "npm run build",
    "prepare": "shx cp -n .env.example .env"
  },
  "dependencies": {
    "axios": "0.19.0",
    "bootstrap": "4.4.1",
    "cookie-parser": "1.4.4",
    "express": "4.17.1",
    "html-minifier": "4.0.0",
    "jquery": "3.4.1",
    "json-stringify-safe": "5.0.1",
    "lodash": "4.17.15",
    "lru-cache": "5.1.1",
    "node-zopfli-es": "1.0.3",
    "nunjucks": "3.2.0",
    "popper.js": "1.16.0",
    "purgecss": "1.4.2",
    "redis": "3.1.1",
    "serve-favicon": "2.5.0",
    "shrink-ray-current": "4.1.2",
    "vue": "2.6.11",
    "vue-observe-visibility": "0.4.6",
    "vue-router": "3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vuex": "3.1.2",
    "vuex-i18n": "1.13.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@babel/plugin-proposal-do-expressions": "7.7.4",
    "@babel/plugin-proposal-export-default-from": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-function-bind": "7.7.4",
    "@babel/plugin-proposal-function-sent": "7.7.4",
    "@babel/plugin-proposal-json-strings": "7.7.4",
    "@babel/plugin-proposal-logical-assignment-operators": "7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.7.4",
    "@babel/plugin-proposal-numeric-separator": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "7.7.7",
    "@babel/plugin-proposal-throw-expressions": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-syntax-import-meta": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "autoprefixer": "9.7.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-remove-webpack": "1.1.0",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "chokidar": "3.3.1",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.0",
    "cross-env": "6.0.3",
    "css-loader": "3.4.0",
    "decache": "4.5.1",
    "del-cli": "3.0.0",
    "env-cmd": "10.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-vue": "6.0.1",
    "file-loader": "5.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "ignore-loader": "0.1.2",
    "image-webpack-loader": "6.0.0",
    "lint-staged": "9.5.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "postcss-object-fit-images": "1.1.2",
    "release-it": "12.4.3",
    "sass-loader": "8.0.0",
    "shx": "0.3.2",
    "style-loader": "1.1.1",
    "stylelint": "12.0.0",
    "stylelint-config-twbs-bootstrap": "1.0.0",
    "stylelint-webpack-plugin": "1.1.2",
    "svg-to-vue-component": "0.3.8",
    "terser-webpack-plugin": "2.3.1",
    "url-loader": "3.0.0",
    "vue-eslint-parser": "7.0.0",
    "vue-loader": "15.8.3",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.4",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-node-externals": "1.7.2",
    "webpack-notifier": "1.8.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "engines": {
    "node": "12.9.1",
    "npm": "6.10.2"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "plugins": [
      "babel-plugin-lodash",
      "@babel/plugin-proposal-function-bind",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-json-strings"
    ],
    "env": {
      "browser": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false,
              "loose": true,
              "useBuiltIns": "usage",
              "corejs": {
                "version": 3,
                "proposals": true
              },
              "targets": {
                "browsers": [
                  ">0.25%",
                  "not ie 10",
                  "not op_mini all",
                  "not dead",
                  "not Android > 0"
                ]
              }
            }
          ]
        ],
        "plugins": [
          "babel-plugin-remove-webpack",
          "@babel/plugin-transform-runtime"
        ]
      },
      "node": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ],
        "plugins": [
          "babel-plugin-dynamic-import-node"
        ]
      }
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-twbs-bootstrap/scss"
    ],
    "rules": {
      "property-blacklist": [
        "border-radius",
        "border-top-left-radius",
        "border-top-right-radius",
        "border-bottom-right-radius",
        "border-bottom-left-radius",
        "transition"
      ]
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "vue-eslint-parser",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "vue",
      "babel"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "allowImportExportEverywhere": false
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
    ],
    "settings": {
      "import/resolver": {
        "node": {},
        "webpack": {
          "config": "webpack/config.base.js"
        }
      }
    },
    "rules": {
      "no-unused-expressions": "off",
      "babel/no-unused-expressions": "error",
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "vue": "never"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "context",
            "config",
            "global",
            "state",
            "acc",
            "req",
            "e"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "webpack/**/*"
          ]
        }
      ],
      "import/prefer-default-export": "off",
      "import/named": "off",
      "import/export": "off",
      "no-debugger": "error",
      "no-console": "error",
      "no-unused-vars": "error"
    },
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "indent": "off",
          "vue/script-indent": [
            "off",
            2,
            {
              "baseIndent": 1
            }
          ]
        }
      }
    ]
  },
  "eslintIgnore": [
    "dist/**/*"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-object-fit-images": {}
    }
  },
  "husky": {
    "hooks": {
      "post-merge": "npm i",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint:scripts -- --fix",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "npm run lint:styles -- --fix",
      "git add"
    ]
  },
  "release-it": {
    "npm": {
      "publish": false
    }
  },
  "nodemonConfig": {
    "exec": "babel-node",
    "watch": [
      "src/server"
    ],
    "ext": "js,njk,json"
  }
}
